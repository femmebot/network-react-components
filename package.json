{
  "name": "ideo-shared",
  "version": "0.1.0",
  "repository": {},
  "license": "UNLICENSED",
  "private": true,
  "main": "dist/main.js",
  "engines": {
    "node": "8.9.0",
    "yarn": "^1.7.0"
  },
  "scripts": {
    "eslint": "eslint .",
    "format": "yarn format:prettier && yarn format:eslint && yarn format:tslint",
    "format:eslint": "eslint --fix .",
    "format:prettier": "prettier --write \"**/*.{ts,tsx,css,json}\" !**/dist/** !**/package.json !**/vendor/**",
    "format:tslint": "tslint --fix --project tsconfig.json",
    "tslint": "tslint --project tsconfig.json",
    "lint": "yarn eslint && yarn tslint",
    "start": "npm run styleguide",
    "styleguide": "yarn styleguidist server",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "webpack:analyze": "ANALYZE=1 NODE_ENV=production bin/webpack",
    "build": "webpack --config webpack/production.js --mode production"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@material-ui/core": "^1.0.0",
    "@types/jest": "^22.2.3",
    "@types/js-cookie": "^2.1.0",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-select": "^2.0.2",
    "@types/react-stripe-elements": "^1.1.6",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "date-fns": "^1.29.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint_d": "^5.3.0",
    "file-loader": "^2.0.0",
    "formik": "^0.11.11",
    "i18n-iso-countries": "^3.7.3",
    "jest": "^22.4.3",
    "jest-styled-components": "^5.0.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.5",
    "ng-in-viewport": "^1.2.0",
    "pluralize": "^7.0.0",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-docgen-typescript": "^1.5.0",
    "react-dom": "^16.3.2",
    "react-popper": "^0.10.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^2.0.0",
    "react-stripe-elements": "^2.0.0",
    "react-styleguidist": "^7.0.14",
    "react-svg-loader": "^2.1.0",
    "react-truncator": "^0.0.1",
    "styled-components": "^3.2.6",
    "ts-jest": "^22.4.4",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.3",
    "typescript-styled-plugin": "^0.6.3",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/lodash": "^4.14.108",
    "@types/react-copy-to-clipboard": "^4.2.5",
    "@types/react-test-renderer": "^16.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "jest-enzyme": "^6.0.0",
    "jest-fetch-mock": "^1.5.0",
    "react-test-renderer": "^16.3.2"
  },
  "jest": {
    "resetMocks": true,
    "automock": false,
    "setupFiles": [
      "<rootDir>/jest/setupJest.js"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(png|svg|otf)$": "<rootDir>/jest/stub.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src",
      "__tests__",
      "<rootDir>"
    ],
    "testRegex": "__tests__/.*\\.test\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://locahost"
  }
}
